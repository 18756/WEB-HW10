ACA,QAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,aAAA;AACA;AACA,KALA;AAMA;AA3BA,CAAA","sourcesContent":["<template>\r\n    <div class=\"middle\">\r\n        <aside>\r\n            <SidebarPost v-for=\"post in viewPosts\" :post=\"post\" :key=\"post.id\"/>\r\n        </aside>\r\n        <main>\r\n            <Index v-if=\"page === 'Index'\" :posts=\"posts\"/>\r\n            <Enter v-if=\"page === 'Enter'\"/>\r\n            <Register v-if=\"page === 'Register'\"/>\r\n            <Users v-if=\"page === 'Users'\" :users=\"users\"/>\r\n            <WritePost v-if=\"page === 'WritePost'\"/>\r\n        </main>\r\n    </div>\r\n</template>\r\n<script>\r\n    import Index from './middle/Index';\r\n    import Enter from './middle/Enter';\r\n    import Register from './middle/Register';\r\n    import SidebarPost from './SidebarPost';\r\n    import Users from './middle/Users';\r\n    import WritePost from './middle/WritePost';\r\n\r\n    export default {\r\n        name: \"Middle\",\r\n        props: [\"posts\", \"users\"],\r\n        data: function () {\r\n            return {\r\n                page: \"Index\"\r\n            }\r\n        },\r\n        computed: {\r\n            viewPosts: function () {\r\n                return Object.values(this.posts).sort((a, b) => b.id - a.id).slice(0, 2);\r\n            }\r\n        },\r\n        components: {\r\n            Index,\r\n            Enter,\r\n            Register,\r\n            SidebarPost,\r\n            Users,\r\n            WritePost\r\n        }, beforeCreate() {\r\n            this.$root.$on(\"onChangePage\", (page) => {\r\n                this.page = page;\r\n                if (this.page === \"Users\") {\r\n                    this.$root.$emit(\"updateUsers\");\r\n                }\r\n            });\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}